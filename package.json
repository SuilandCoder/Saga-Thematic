{
  "name": "saga-thematic",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.config.json -o",
    "build:aot": "ng build --aot --base-href ./ --output-path=dist-aot",
    "build": "ng build --prod --build-optimizer --base-href ./ --output-path=dist --delete-output-path",
    "build:prod": "node --max_old_space_size=5120 node_modules/@angular/cli/bin/ng build  --base-href ./ --output-path=build-prod --delete-output-path --prod --build-optimizer=false --watch --common-chunk --vendor-chunk",
    "build:prod:optimizer": "node --max_old_space_size=5120 node_modules/@angular/cli/bin/ng build --base-href ./ --output-path=build-prod-optimizer --delete-output-path --prod --build-optimizer --watch --common-chunk --vendor-chunk --stats-json",
    "bundle-report": "webpack-bundle-analyzer dist/Saga-Thematic/stats.json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
